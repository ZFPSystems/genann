# Project:   !Genann


# Toolflags:
CCflags = -c -depend !Depend -throwback         -strict -fa  -fh     -Ospace  -apcs 3/32/fpe2/swst/fp/nofpr
C++flags = -c -depend !Depend -IC: -throwback
Linkflags = -aif -c++ -o $@
ObjAsmflags = -throwback -NoCache -depend !Depend
CMHGflags =
LibFileflags = -o  -c  $@
Squeezeflags = -o $@
ModSqzflags = -o $@


# Final targets:
@.^.o.genann: @.o.genann
           libfile $(LibFileflags) @.o.genann


# User-editable dependencies:


# Static dependencies:
@.o.genann: @.c.genann
          cc $(CCflags) -o @.o.genann @.c.genann


# Dynamic dependencies:

o.genann:	c.genann
o.genann:	h.genann
