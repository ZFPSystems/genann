# Project:   !Genann


# Toolflags:
CCflags = -O3 -mthrowback -c -Wl,-s -shared -fPIC -IRiscLuaSrc: -llua
CMHGflags =
LibFileflags = -o  -c  $@
Squeezeflags = -o $@
ModSqzflags = -o $@


# Final targets:
$@.^.o.GenannLua/so: o.GenannLua/so
	copy @.o.GenannLua/so @.^.so.GenannLua/so/1/0/0 ~C F
	SetType @.^.so.GenannLua/so/1/0/0 ELF


# User-editable dependencies:


# Static dependencies:
o.GenannLua/so: c.genannLua
	gcc $(CCflags) -o $@ c.genannLua


# Dynamic dependencies:
o.GenannLua/so: c.genannLua
