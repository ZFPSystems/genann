# Project:   !Genann


# Toolflags:
CCflags = -O3 -mthrowback -c -Wl,-s -shared -fPIC
CMHGflags =
LibFileflags = -o  -c  $@
Squeezeflags = -o $@
ModSqzflags = -o $@


# Final targets:
$@.^.o.libGenann/so: o.libGenann/so
	copy @.o.libGenann/so @.^.so.libGenann/so/1/0/0 ~C F
	SetType @.^.so.libGenann/so/1/0/0 ELF


# User-editable dependencies:


# Static dependencies:
o.libGenann/so: c.genann
	gcc $(CCflags) -o $@ c.genann


# Dynamic dependencies:
o.libGenann/so: c.genann
